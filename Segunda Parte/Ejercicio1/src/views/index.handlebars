<h1>Bienvenido, {{user.name}}</h1>

{{#if isAdmin}}
  <h2>Lista de Alimentos</h2>
  {{#each food}}
    <li>
      {{this.name}}
      - ${{this.price}}
    </li>
  {{/each}}

{{else}}
  <p>
    No tienes permisos para ver la lista de Alimentos
  </p>
{{/if}}

